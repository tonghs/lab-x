/**app.wxss**/
page {
  background-color: #fafafa;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.line {
  width: 100%;
  height: 1rpx;
  background: #cdcdcd60;
}

.flex-row,
.flex-column {
  display: flex;
}

.flex-column {
  flex-direction: column;
}

.flex-row {
  flex-direction: row;
}

.footer {
  font-weight: 300;
  color: #cdcdcd;
  font-size: 24rpx;
  width: 100%;
  text-align: center;

}

.footer .line {
  margin-bottom: 40rpx;
}

.bottom-footer {
  position: absolute;
  bottom: 60rpx;
}

.full-width {
  width: 100%;
}

.linked {
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-position: center right -10rpx;
  background-size: 40rpx;
  padding-right: 40rpx;
}


.card:first-child {
  margin-top: 0;
}

.rounded:first-child {
  margin-top: 20rpx;
}

.card {
  padding: 30rpx 30rpx 30rpx;
  margin: 15rpx 0 15rpx;
  /* border: 1px solid #cdcdcd60; */
  background-color: #fff;
  border-width: 1rpx 0;
}

.rounded {
  margin: 15rpx 20rpx 15rpx;
  border-radius: 20rpx;
  box-shadow: 0 0 3rpx #cdcdcd60;
  border-width: 0;
}

.card .title {
  flex-direction: row;
  font-size: 25rpx;
  justify-content: space-between;
}

.card .linked-secondary {
  font-size: 25rpx;
  background-size: 30rpx;
  display: flex;
  align-items: center;
  line-height: 30rpx;
  color: #cdcdcd;
  padding-right: 30rpx;
  padding: 15rpx 30rpx 15rpx;
}

.card .body {
  font-size: 30rpx;
  font-weight: 300;
  padding-top: 10rpx;
  align-items:flex-start;
  justify-content: space-between;
}

.card .empty-body {
  text-align: center;
  margin-top: 20rpx;
}

.card .footer {
  margin-top: 25rpx;
}

.tips {
  display: flex;
  justify-content: left;
  align-items: center;
}

.tips .item {
  display: flex;
  justify-content: left;
  align-items: center;
  margin-right: 25rpx;
}

.tips .item .label{
  margin-left: 10rpx;
  font-size: 25rpx;
  color: #00000080;
}

.normal-bg-color {
  background-color: #91CB74;
}

.warn-bg-color {
  background-color: #EE6666;
}

.normal-color {
  color: #91CB74;
}

.warn-color {
  color: #EE6666;
}

.default-color {
  color: #000000;
}

.dot {
  display: block;
  width: 10rpx;
  height: 10rpx;
  padding: 1rpx;
  border-radius: 10rpx;
}

.secondary {
  color: #cdcdcd;
}


.label-container {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 0rpx;
  background-color: #fff;
}

.label-container .label {
  margin: auto;
  text-align: center;
  border-radius: 10rpx;
  width: 100%;
  padding: 8rpx 0;
  margin: 0 20rpx;
  box-shadow: 0 0 5rpx #00000050;
  font-size: 30rpx;
}

.disabled {
  background-color: #cdcdcd40;
  color: #00000080;
}

.active {
  background-color: #cdcdcd80;
  color: #000000;
}

/* switch */
.wx-switch-input{
  width: 82rpx!important;
  height: 40rpx!important;
}
/*关闭状态*/
.wx-switch-input::before{
  width: 80rpx!important;
  height: 32rpx!important;
}
/*开启状态*/
.wx-switch-input::after{
  width: 38rpx!important;
  height: 32rpx!important;
}
