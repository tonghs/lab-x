/**index.wxss**/
.userinfo {
  background-color: #ffffff;
  display: flex;
  flex-direction: row;
  width: 100%;
  padding: 20px 0;
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-position: center right 20rpx;
  background-size: 40rpx;
}

.nickname, .avatar {
  margin-right: 15rpx;
}

.nickname {
  padding-top: 26rpx;
}

.line {
  margin: 20rpx 0;
}
.userinfo-avatar {
  width: 100rpx;
  height: 100rpx;
  margin: 0 20rpx 0;
  border-radius: 50%;
}

.userinfo-nickname {
  font-size: 35rpx;
}

view.account {
  width: 100%;
  text-align: center;
  padding: 50rpx 0;
}

.settings {
  margin: 0 0 20rpx;
  width: 100%;
  background-color: #ffffff;
}

.settings-item:first-child {
  border-top: 1px solid #cdcdcd60;
}

.settings-item {
  display: inline-flex;
  flex-direction: row;
  padding: 25rpx 20rpx;
  align-items: center;
  border-bottom: 1px solid #cdcdcd60;
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-position: center right 10px;
  background-size: 40rpx;
}

.setting-icon {
  width: 30rpx;
  height: 30rpx;
  margin: 0 15rpx;
}

.setting-icon-txt {
  font-size: 27rpx;
  padding-top: 5rpx;
}

.logout {
  font-size: 30rpx;
  width: 100%;
  text-align: center;
  padding: 25rpx 0;
  margin: 20rpx 0 0;
}

.footer {
  position: absolute;
  bottom: 40rpx;
}